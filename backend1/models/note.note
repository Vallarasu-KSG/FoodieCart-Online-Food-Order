const mongoose = require('mongoose');

const FoodModelName = new mongoose.Schema(
    {
        name:
        {
            type:String,
            required:true
        },
        image:
        {
            type:String,
            required:true
        },
        price:
        {
            type:Number,
            required:true
        },
        offerPrice:
        {
            type:Number,
            required:true
        },
        category:
        {
            type:String,
            required:true
        },
        address:
        {
            type:String,
            required:true
        }
    }
)


module.exports = mongoose.models.food || mongoose.model("food",FoodModelName)